 int majorityElement(vector<int> &num) 
    {
        //bit manipulation. code not simplified for readability
        int countsPerBit[32];
        int result = 0;
        for (int i=0; i<32; i++)
        {
            int countone = 0;
            int countzero = 0;
            
            for (int j=0; j<num.size(); j++) 
            {
                if((num[j]>>i&1) ==1) 
                    countone++;
                else 
                  countzero++;
            }
            
            if (countone>=countzero) 
                result |= (1<<i);
            else 
                result |= (0<<i);
        }
        return result;
        
        //Mooreâ€™s Voting Algorithm. O(n)
        /*
